{
  "env": {
      "development": {
        "presets": ["react","es2015","stage-0"],
        "plugins": ["syntax-object-rest-spread","syntax-function-bind"]
      },
      "default": {
        "presets": ["react","es2015","stage-0"],
        "plugins": ["syntax-object-rest-spread","syntax-function-bind"]
      },
      "es": {
        "plugins": ["syntax-object-rest-spread","syntax-function-bind"],
        "presets": [
          "react",
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0"
        ]
      }
    }
}